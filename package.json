{
  "name": "formalterm",
  "version": "0.0.4",
  "description": "Another electron terminal emulator.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "rebuild-node-pty": "electron-rebuild -f -o node-pty -m target",
    "rebuild": "electron-rebuild -f -o node-pty -m",
    "dist": "electron-builder",
    "distintel": "electron-builder --x64 --arm64"
  },
  "repository": "https://github.com/FormalSnake/Formalterm.git",
  "keywords": [
    "Electron",
    "Terminal"
  ],
  "build": {
    "appId": "com.kyandesutter.formalterm"
  },
  "author": "Kyan De Sutter",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^23.1.1",
    "electron-builder": "^23.6.0",
    "electron-rebuild": "^3.2.9"
  },
  "dependencies": {
    "electron-preferences": "^2.8.1",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0",
    "node-pty": "^0.10.1",
    "v8-compile-cache": "^2.3.0",
    "xterm": "^5.1.0",
    "xterm-addon-canvas": "^0.3.0",
    "xterm-addon-fit": "^0.7.0",
    "xterm-addon-image": "^0.4.0",
    "xterm-addon-ligatures": "^0.6.0",
    "xterm-addon-webgl": "^0.14.0"
  }
}
